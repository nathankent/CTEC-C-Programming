#include <stdio.h>

int main()
{
    int v1, v2, v3;
    scanf("%i %i %i",&v1,&v2,&v3);
    
    
    if (v1==1)
    {
        if (v2==1)
        	if (v3==1)
           printf("A ");
        else
           printf("B ");
    }

    else
    {
        if (v2==1)
        	if (v3==1)
           printf("C ");
        else
           printf("D ");
    }
    printf("\n");
    
    
    
   
    
    return 0;
}
