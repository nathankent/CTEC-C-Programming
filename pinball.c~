#include <stdio.h>

int main()
{
    int v1, v2, v3, v4, top, bottom;
    scanf("%i %i %i %i",&v1,&v2,&v3,&v4);
    
    // Part 1 //
    
    top = v1;
    bottom = v2;
    
    if (top==1)
    	   {
    
 	   if (v1==1)
 	   {
 	   	   if (v2==1) {
 	   	   	if (v3==1) 
 	   	   			printf("A");
 	   	   		else 
 	   	   			printf("B");
 	   	   	      }
 	    
 	   }
 	
 	   else
 	   {
 	       if (v2==1)
 	          printf("C ");
 	       else
 	          printf("D ");
 	   }
 	   
 	   }
    // Part 2 //
    
    if (bottom==1)
    {
    
 	   if (v1!=1)
 	   {
 	   	   if (v2==1) {
 	   	   	if (v3==1) 
 	   	   			printf("E");
 	   	   		else 
 	   	   			printf("F");
 	   	   	      }
 	    
 	   }
 	
 	   else
 	   {
 	       if (v2==1)
 	          printf("G");
 	       else
 	          printf("H");
 	   }
 	   
 	   }
    

    printf("\n");
    return 0;
}
